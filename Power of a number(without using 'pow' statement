#include <iostream>
#include <math.h>
using namespace std;
int main(){
        int a,b;

     cout<<"Please enter the number : ";
     cin>>a;
     cout<<"Please enter the power : ";
     cin>>b;
     
     int ans=1;

     for(int i=1; i<=b; i++){
        ans=ans*a;
     }
     cout<<"The answer is "<<ans<<endl;
}
