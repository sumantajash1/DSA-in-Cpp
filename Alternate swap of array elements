#include <iostream>
#include <math.h>
using namespace std;
void reverse(int array[], int size){   
    
       for(int i=0; i<size; i+=2){
           if(i+1<size){
              swap(array[i],array[i+1]);
           }
       }
   }

void Printarray(int array[], int size){
    for(int i=0; i<size; i++){
         cout<<array[i]<<" ";
    }
    cout<<endl;
}
int main(){
     int array[6]={1,2,3,4,5};
     reverse(array,5);
     Printarray(array,5);
}
