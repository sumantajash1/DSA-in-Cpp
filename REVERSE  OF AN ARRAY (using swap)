#include <iostream>
#include <math.h>
using namespace std;
void reverse(int array[],int size){
      int start=0;
      int end=size-1;
      while(start<=end){
          swap(array[start],array[end]);
          start++;
          end--;
      }
}

void Printarray(int array[], int size){
      for(int i=0; i<size; i++){
          cout<<array[i]<<endl;
      }
}
int main(){
     int array[5]={1,2,3,4,5};
     reverse(array,5);
     Printarray(array,5);
}
