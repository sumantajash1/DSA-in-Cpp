#include <iostream>
#include <math.h>
using namespace std;

int main(){
	 int n;
	 cout<<"Please enter the number of termes : ";
	 cin>>n;
	 int t1=0;
	 int t2=1;
	 int nxt;
	 cout<<"The Fibonacci series is : ";
     for(int i=1; i<=n; i++){
		   if(i==1){
			    cout<<t1<<", ";
				continue;
		   }
		   if(i==2){
			   cout<<t2<<", ";
			   continue;
		   }
		   nxt=t1+t2;
		   t1=t2;
		   t2=nxt;
		   cout<<nxt<<", ";

	 }
}
