#include<iostream>
#include<math.h>
using namespace std;

int reverse(int array[], int size){
         int storage[size];
    
        //  while(i<=size){
             
        //  }
        for(int i=0; i<size; i++){
              storage[i]=array[size-1-i];
        }
         for(int i=0; i<size; i++){
              cout<<storage[i]<<" ";
         }
}

int main(){
       int array[5]={1,2,3,4,5};
     //   int size;
     //   cout<<"Please enter the size of the array : ";
     //   cin>>size;
       cout<<"Please enter the elements of the arrray : "<<endl;
       for(int i=0; i<5; i++){
             cin>>array[i];
       }
      reverse(array,5);
         
}
