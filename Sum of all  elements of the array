
#include <iostream>
#include <math.h>
using namespace std;

int SUM(int array[], int size){
         int sum=0;
         for(int i=0; i<size; i++){
              sum=sum+array[i];
         }
         return sum;
  }

  int main(){
       int size;
       cout<<"Please enter the size of the array : ";
       cin>>size;
        int array[100];
         for(int i=0; i<size; i++){
            cin>>array[i];
         }
         int answer=SUM(array,size);
         cout<<"The Sum of all elements of the array is : "<<answer;

         
  }
