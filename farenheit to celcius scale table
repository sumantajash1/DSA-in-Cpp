#include <iostream>
#include <math.h>
using namespace std;
int main(){
     int startfahrenheit=0;
    int endfahrenheit=100;


    cout<< "fahrenheit\tcelcius" <<endl;
     int fahrenheit = startfahrenheit;
    while (fahrenheit<=endfahrenheit)
    {
        double celcius = (fahrenheit - 32) * 5.0 / 9.0;

        cout<< fahrenheit << "\t\t" << celcius << endl;

        fahrenheit = fahrenheit+1; 
    }

}
