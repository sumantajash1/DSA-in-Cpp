#include <iostream>
#include <math.h>
using namespace std;
int main(){
       int power(int a, int b){

        int ans=1;
        for(int i=1; i<=b; i++){
           ans=ans*a;
        }
        return ans;
      }

     int main(){
        int a,b;
        cout<<"Please enter the value of a : ";
        cin>>a;
        cout<<"Please enter the value of b : ";
        cin>>b;
        int answer=power(a,b);
        cout<<"The ans is : "<<answer<<endl;
        return answer;
      }

}
